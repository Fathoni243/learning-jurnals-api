  "license": "ISC",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeds/seed.ts"
  },
  "homepage": "https://github.com/Fathoni243/learning-jurnals-api.git",
  "devDependencies": {
